{{> layout/header }}

<div class="bg-light py-5">
    <div class="weapp-container-person-list">
        <div class="weapp-main-wrapper-person-list">
            <div class="weapp-profile-grid-person-list">
                {{#models}}
                    <div class="weapp-profile-card-person-list">
                        <img src="#" class="weapp-company-logo-person-list"/>

                        <div class="weapp-company-name-person-list"><a class="weapp-pagination-person-list" href="">{{user.username}}</div>
                        <div class="weapp-company-desc-person-list">{{title}}</div>
                    </div>
                {{/models}}
            </div>
        </div>

        <!-- 사이드바 -->
        <div class="weapp-sidebar-person-list">
            <!-- <div class="weapp-register-box">
    <button>기업 프로필 무료 등록</button>
  </div> -->
            <div class="weapp-search-box-person-list">
                <h3>사람 검색</h3>
                <div class="weapp-filters-person-list"></div>
                <input type="text" placeholder="검색어 입력"/>
            </div>
        </div>
    </div>

    <!-- 페이지네이션 -->
    <div class="weapp-pagination-person-list">
        <button class="weapp-active-person-list"><</button>
        <button>1</button>
        <button>2</button>
        <button>3</button>
        <button>...</button>
        <button>5</button>
        <button>></button>
    </div>
</div>

{{> layout/footer }}